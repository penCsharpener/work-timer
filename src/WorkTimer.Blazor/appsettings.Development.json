{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SqliteConfiguration": {
    "PathToDatabase": "%appdata%\\WorkTimer",
    "DatabaseFileName": "worktimer.db",
    "PathToIdentityDatabase": "%appdata%\\WorkTimer\\identity.db"
  },
  "ConnectionStrings": {
    "SqliteEFCoreIdentity": "Data Source=%appdata%\\WorkTimer\\identity.db;"
  }
}